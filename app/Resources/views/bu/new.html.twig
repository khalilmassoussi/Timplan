{% extends 'Squelette.html.twig' %}
{% block css %}
    {{ parent() }}
    <script src="{{ asset('Template/dist/js/pages/dashboard2.js') }}"></script>
{% endblock %}
{% block Titre %}
    Portail web de TimSoft - Gestion des Business units
{% endblock %}
{% block Navigation %}
    <li class="active">Création d'un Business units</li>
{% endblock %}
{% block Contenu %}
    <div class="col-md-9">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h2 class="box-title" style="color:#1E5584">Création d'un Business Units</h2>
            </div>
            <div class="box-body">
                {{ form_start(form) }}
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Libellé</label>
                            {{ form_widget(form.libelle , {'attr' : {'class' : 'form-control' , 'placeholder' : 'Libellé...'}} ) }}
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Utilisateurs</label>
                            {{ form_widget(form.utilisateurs, {'attr' : {'class' : 'form-control'}} ) }}
                        </div>
                    </div>
                </div>
                <div class="form-group" style="padding: 15px 15px 0px 15px">
                    <button type="reset" class="btn btn-primary btn-block btn-flat">Annuler</button>
                </div>
                <div class="form-group" style="padding: 0px 15px 20px 15px">
                    <button type="submit" class="btn btn-primary btn-block btn-flat">Enregister</button>
                </div>
                {{ form_widget(form._token) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>

{% endblock %}
