    <script>
        function editPlanningModal(event, calendar, data) {
            var id;
            {% if not AutorisationAcces('modifinterv' , app.user) %}
            if (typeof event.event !== 'undefined') {
                if (typeof event.event._def.extendedProps.resourceIds !== 'undefined') {
                    idU = event.event._def.extendedProps.resourceIds[0];
                } else if (typeof event.event._def.resourceIds !== 'undefined') {
                    idU = event.event._def.resourceIds[0];
                }
                statut = event.event.extendedProps.Statut;
                id = event.event.id;
            } else {
                idU = data.resourceIds[0];
                statut = data.Statut;
                id = data.id;
            }
            console.log(statut);
            console.log(id);
            if (idU.toString() !== '{{ app.user.id }}' || statut === 'Terminé') {
                $.ajax({
                    url: Routing.generate('showPlanning', {id: id}),
                    method: 'post',
                    // dataType: 'json',
                    success: function (data) {
                        $("#renderEdit").html(data);
                        $('#showPlanning').modal();
                    },
                });
            } else {
                $.ajax({
                    url: Routing.generate('editPlanning', {id: id}),
                    method: 'post',
                    // dataType: 'json',
                    success: function (data) {
                        $("#renderEdit").html(data);
                        $('#editPlanning').modal();
                        $('#editPlanning').on('hidden.bs.modal', function () {
                            calendar.refetchEvents();
                        });
                    },
                });
            }
            {% else %}
            var idPlanning;
            if (typeof event.event === 'undefined') {
                idPlanning = data.id;
            } else {
                idPlanning = event.event.id;
            }
            $.ajax({
                url: Routing.generate('editPlanning', {id: idPlanning}),
                method: 'post',
                success: function (data) {
                    $("#renderEdit").html(data);
                    $('#editPlanning').modal();
                    $('#editPlanning').on('hidden.bs.modal', function () {
                        calendar.refetchEvents();
                    });
                },
            });
            {% endif %}

        }

        function editTaskEvent(event, calendar) {
            {% if not AutorisationAcces('modifTaskEvent' , app.user) %}
            var id;
            if (typeof event.event._def.extendedProps.resourceIds !== 'undefined') {
                id = event.event._def.extendedProps.resourceIds[0];
            } else {
                id = event.event._def.resourceIds[0];
            }
            if (id !== '{{ app.user.id }}' || event.event.extendedProps.Statut === 'Done') {
                $.ajax({
                    url: Routing.generate('taskevent_show', {id: event.event.id}),
                    method: 'get',
                    // dataType: 'json',
                    success: function (data) {
                        $("#renderEdit").html(data);
                        $('#showTask').modal();
                        $('#showTask').on('hidden.bs.modal', function () {
                            calendar.refetchEvents();
                        });
                    },
                });
            } else {
                $.ajax({
                    url: Routing.generate('taskevent_edit', {id: event.event.id}),
                    method: 'post',
                    // dataType: 'json',
                    success: function (data) {
                        $("#renderEdit").html(data);
                        $('#editTask').modal();
                        $('#editTask').on('hidden.bs.modal', function () {
                            calendar.refetchEvents();
                        });
                    },
                });
            }
            {% else %}
            $.ajax({
                url: Routing.generate('taskevent_edit', {id: event.event.id}),
                method: 'post',
                // dataType: 'json',
                success: function (data) {
