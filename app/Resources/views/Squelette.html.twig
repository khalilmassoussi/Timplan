{% block body %}
    {% set Rubriques = afficherRubrique(app.user) %}
    {% set Utilisateur = app.user %}
    {% set role = app.user.roleUtilisateur %}
    {% set NotificationNonVu = NotificationNonVu(app.user) %}
    {% set Notifications = Notifications(app.user) %}
    {% if ( role == 'ROLE_ADMIN') %} {% set RoleUser = 'Administrateur' %}
    {% elseif ( role == 'ROLE_CONSULTANT') %}{% set RoleUser = 'Consultant' %}
    {% elseif ( role == 'ROLE_CLIENT') %}{% set RoleUser = 'Client' %}
    {% elseif ( role == 'ROLE_GESTIONNAIRE') %}{% set RoleUser = 'Gestionnaire' %}
    {% elseif ( role == 'ROLE_CHEF') %}{% set RoleUser = 'BU Manager' %}
    {% endif %}
    {% for message in app.flashes('notice') %}
        <div class="flash-notice">
            <body onLoad="$('#my-modal').modal('show');">
            <div id="my-modal" class="modal fade">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">Notice</h4>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        </div>
                        <div class="modal-body">
                            {{ message }}
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Fermer</button>
                        </div>
                    </div>
                </div>
            </div>
            </body>
        </div>
    {% endfor %}
    <body class="skin-blue sidebar-mini pace-primary pace-done">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">

                    <a href="{{ path('tim_soft_client_homepage') }}" class="nav-link">Accueil</a>
                </li>
                {#<li class="nav-item d-none d-sm-inline-block">#}
                {#<a href="#" class="nav-link">Contact</a>#}
                {#</li>#}
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-bell"></i>
                        <span class="badge badge-warning navbar-badge">{{ NotificationNonVu|length }}</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <span class="dropdown-item dropdown-header">{{ NotificationNonVu|length }} nouvelle(s) notification(s)</span>
                        {% for Notification in Notifications[:3] %}
                            {% if (Notification.vu == 0) %}
                                <div class="dropdown-divider"></div>
                                <a href="{{ path(Notification.Notification.route, Notification.Notification.parameters) }}"
                                   class="dropdown-item" onclick="ModifVu({{ Notification.id }})">
                                    {#                                <div class="card">#}
                                    {#                                    <div class="card-header">#}
                                    {#                                        {{ Notification.Notification.title }}#}
                                    {#                                    </div>#}
                                    {#                                    <div class="card-body">#}

                                    {#                                    </div>#}
                                    {#                                    <div class="card-footer">#}
                                    {#                                        <span class="float-right text-muted text-sm">{{ Notification.Notification.date|date('d-m-Y H:i') }}</span>#}
                                    {#                                    </div>#}
                                    {#                                </div>#}
                                    <div class="media">
                                        {% if app.user.photoDeProfil == null %}
                                            <img src="{{ asset('Template/user.png') }}"
                                                 class="img-size-50 mr-3 img-circle"
                                                 alt="Photo De profil">
                                        {% else %}
                                            <img src="{{ asset('uploads/PhotoDeProfil/') }}{{ app.user.photoDeProfil }}"
                                                 class="img-size-50 mr-3 img-circle" alt="Photo De profil">
                                        {% endif %}
                                        {#                                    <img src="../../dist/img/user1-128x128.jpg" alt="User Avatar" class="img-size-50 mr-3 img-circle">#}
                                        <div class="media-body">
                                            <h3 class="dropdown-item-title">
                                                {{ Notification.Notification.responsable }}
                                                <span class="float-right text-sm text-danger"></span>
                                            </h3>
                                            <p class="text-sm">{{ Notification.Notification.title }}
                                            </p>
                                            <p class="text-sm text-muted"><i
                                                        class="far fa-clock mr-1"></i> {{ Notification.Notification.date|date('d-m-Y H:i') }}
                                            </p>
                                        </div>
                                    </div>
                                </a>
                            {% endif %}
                        {% endfor %}
                        <a href="{{ path('AllNotifications') }}" class="dropdown-item dropdown-footer">Voir Toutes</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#">
