
    <h1>Taskevent</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ taskEvent.id }}</td>
            </tr>
            <tr>
                <th>Site</th>
                <td>{{ taskEvent.site }}</td>
            </tr>
            <tr>
                <th>Etiquette</th>
                <td>{{ taskEvent.etiquette }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ taskEvent.statut }}</td>
            </tr>
            <tr>
                <th>Motif</th>
                <td>{{ taskEvent.motif }}</td>
            </tr>
            <tr>
                <th>Progression</th>
                <td>{{ taskEvent.progression }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{% if taskEvent.start %}{{ taskEvent.start|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{% if taskEvent.end %}{{ taskEvent.end|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('taskevent_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('taskevent_edit', { 'id': taskEvent.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>

