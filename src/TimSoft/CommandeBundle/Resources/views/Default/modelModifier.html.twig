        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="title">Modification</h4>
            </div>
            <div class="modal-body" id="modifierCModalBody">
                {{ form_start(form, { 'method' : 'post', 'action': path('modifierC', { 'id' : commande.id }) }) }}
                {{ form_row(form.nCommande, {'attr': {'class': 'form-control'}}) }}
                {{ form_row(form.client, {'attr': {'class': 'form-control'}}) }}
