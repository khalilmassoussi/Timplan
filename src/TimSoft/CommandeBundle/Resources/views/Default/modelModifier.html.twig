                {{ form_row(form.buManager, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary">Sauvegarder</button>
            </div>
        </div>
    </div>
    {{ form_end(form) }}
</div>
